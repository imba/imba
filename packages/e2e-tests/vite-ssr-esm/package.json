{
  "name": "e2e-tests-vite-ssr-esm",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development imba launch.imba --esm --",
    "build": "run-s build:client build:server",
    "build:client": "vite build -c vite.config.client.js --outDir dist",
    "build:server": "vite build -c vite.config.server.js --outDir dist_server",
    "preview": "cross-env NODE_ENV=production node dist_server/server",
    "debug": "node --inspect-brk dist/server/server"
  },
  "devDependencies": {
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "decamelize": "^6.0.0",
    "express": "^4.18.1",
    "imba": "../../imba",
    "npm-run-all": "^4.1.5",
    "serve-static": "^1.15.0",
    "vite": "^3.1.0",
    "vite-plugin-imba": ":../../vite-plugin-imba"
  },
  "dependencies": {
    "vite-node": "^0.23.1"
  }
}
