	(typeof node == 'string' or typeof node == 'number') ? node : node.c(opts)

# wrap output in path markers
var MP = do |val,typ = 'path'|
	return val if STACK.tsc
	return "{val}/*${typ}$*/"

var MLOC = do |a,b|
	b = a if b == undefined

	{
		startLoc: do a
		endLoc: do b 
	}

var M = do |val,mark,o|
	if mark == undefined
		mark = val

	if mark and mark:startLoc
		val = C(val,o)
		let ref = STACK.incr('sourcePair')
		let start = mark.startLoc
		let end = mark.endLoc
		let m0 = ''
		let m1 = ''

		# if o and o:locRef 
		#	ref += '|' + o:locRef

		if start == 0 or start > 0
			m0 = end >= start ? "/*%{start}|{ref}$*/" : "/*%{start}$*/"
			
		if end == 0 or end > 0
			m1 = start >= 0 ? "/*%{end}|{ref}$*/" : "/*%{end}$*/"
		if o and o:locRef
			val = MP(val,o:locRef)
		return m0 + val + m1
	return C(val,o)

var MSTART = do |*params|
	for item in params
		if item isa Number
			return item
		if item and item:startLoc isa Function
			return item.startLoc
	return null

var MEND = do |*params|
	for item in params
		if item isa Number
			return item
		if item and item:endLoc isa Function
			return item.endLoc
	return null

var TYP = do |item,format|
	let typ = item.@options and item.@options:datatype
	typ ||= item:datatype isa Function and item.datatype

	if typ
		if format == 'jsdoc'
			"/** @type \{{M(typ,typ)}\} */"
		else
			":{M(typ,typ)}"
	else
		""

