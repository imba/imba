	def visit
		if @value.type == 'HERECOMMENT'
			let raw = @value.@value
			let line = raw.slice(0,raw.indexOf('\n')).trim

			if let m = line.match(/^(css|less|stylus|sass|scss)( scoped)?/)
				var style = {
					content: raw.slice(raw.indexOf('\n'))
					scoped: !!m[2]
