var C = do |node,opts|
	typeof node == 'string' ? node : node.c(opts)

var M = do |val,mark,o|
	if mark == undefined
		mark = val

	if mark and mark:startLoc
		val = C(val,o)
		let ref = STACK.incr('sourcePair')
		let start = mark.startLoc
		let end = mark.endLoc
		let m0 = ''
		let m1 = ''
		if start == 0 or start > 0
			m0 = "/*%{start}|{ref}$*/"
		if end == 0 or end > 0
			m1 = "/*%{end}|{ref}$*/"
		return m0 + val + m1
	return C(val,o)

